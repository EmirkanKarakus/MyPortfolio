@model Contact
<section id="contact" class="s-contact target-section">

    <div class="overlay"></div>

    <div class="row narrow section-intro">
        <div class="col-full">
            <h3>İletişim</h3>
            <h1>Bana Ulaşın.</h1>

            <p class="lead">İletişim bilgilerim altta listelenmiştir. Site üzerinden mesaj yazmayıda tercih edebilirsiniz.</p>
        </div>
    </div>

    <div class="row contact__main">
        @await Component.InvokeAsync("_MessageComponentPartial")
        <div class="col-four tab-full contact__infos">
            <h4 class="h06">Telefon</h4>
            <p>
                @ViewBag.v1: (@ViewBag.v2) @ViewBag.v3<br>
                @ViewBag.v1: (@ViewBag.v2) @ViewBag.v4
            </p>

            <h4 class="h06">Email</h4>
            <p>
                @ViewBag.v5<br>
                @ViewBag.v6
            </p>

            <h4 class="h06">Adres</h4>
            <p>
                @ViewBag.v7
            </p>
        </div>

    </div>

</section>